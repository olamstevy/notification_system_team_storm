generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model template {
  id String   @id @default(uuid())
  code String
  language String
  content String
  version Int @default(1)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@unique([code, version]) 
}
